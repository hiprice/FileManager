<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="fileUploadAction" class="com.ccland.action.FileUpLoadAction"
		abstract="false" scope="prototype" lazy-init="default" autowire="default">

	</bean>
	<bean id="fileDownloadAction" class="com.ccland.action.FileDownloadAction"
		abstract="false" scope="prototype" lazy-init="default" autowire="default">

	</bean>

	<bean id="monitorAction" class="com.ccland.action.FastDFSMonitorAction"
		abstract="false" scope="prototype" lazy-init="default" autowire="default">

	</bean>


	<bean id="fileDeleteAction" class="com.ccland.action.FileDeleteAction"
		abstract="false" scope="prototype" lazy-init="default" autowire="default">
	</bean>
</beans>